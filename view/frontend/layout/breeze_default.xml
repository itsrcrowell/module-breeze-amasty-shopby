<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="breeze.js">
            <arguments>
                <argument name="bundles" xsi:type="array">
                    <item name="dynamic" xsi:type="array">
                        <item name="items" xsi:type="array">
                            <item name="amShopbyHelpers" xsi:type="array">
                                <item name="path" xsi:type="string">Amasty_Shopby/js/utils/helpers</item>
                            </item>

                            <item name="amShopbyFilterAbstract" xsi:type="array">
                                <item name="path" xsi:type="string">Amasty_Shopby/js/amShopby</item>
                            </item>

                            <item name="amShopbyFilterContainer" xsi:type="array">
                                <item name="path" xsi:type="string">Swissup_BreezeAmastyShopby/js/components/amShopbyFilterContainer</item>

                                <item name="import" xsi:type="array">
                                    <item name="amShopbyFilterAbstract" xsi:type="string">amShopbyFilterAbstract</item>
                                    <item name="amShopbyFiltersSync" xsi:type="string">amShopbyFiltersSync</item>
                                </item>
                            </item>

                            <item name="amShopbyFilterFromTo" xsi:type="array">
                                <item name="path" xsi:type="string">Swissup_BreezeAmastyShopby/js/components/amShopbyFilterFromTo</item>

                                <item name="import" xsi:type="array">
                                    <item name="amShopbyFilterAbstract" xsi:type="string">amShopbyFilterAbstract</item>
                                    <item name="amShopbyHelpers" xsi:type="string">amShopbyHelpers</item>
                                    <item name="amShopbyFiltersSync" xsi:type="string">amShopbyFiltersSync</item>
                                </item>
                            </item>

                            <item name="amShopbyFiltersSync" xsi:type="array">
                                <item name="path" xsi:type="string">Swissup_BreezeAmastyShopby/js/amShopbyFiltersSync</item>
                            </item>

                            <item name="amShopbyTopFilters" xsi:type="array">
                                <item name="path" xsi:type="string">Swissup_BreezeAmastyShopby/js/amShopbyTopFilters</item>
                            </item>

                            <item name="amShopbyResponsive" xsi:type="array">
                                <item name="path" xsi:type="string">Swissup_BreezeAmastyShopby/js/amShopbyResponsive</item>

                                <item name="import" xsi:type="array">
                                    <item name="amShopbyTopFilters" xsi:type="string">amShopbyTopFilters</item>
                                </item>
                            </item>

                            <item name="Amasty_Shopby/js/components/amShopbyFilterCollapse" xsi:type="array">
                                <item name="path" xsi:type="string">Amasty_Shopby/js/components/amShopbyFilterCollapse</item>

                                <item name="export" xsi:type="array">
                                    <item name="amShopbyFilterCollapse" xsi:type="string">amShopbyFilterCollapse</item>
                                </item>
                            </item>

                            <item name="amShopbySwiperSlider" xsi:type="array">
                                <item name="path" xsi:type="string">Swissup_BreezeAmastyShopby/js/components/amShopbySwiperSlider</item>
                            </item>

                            <item name="Amasty_Shopby/js/components/amShopbyFilterHideMoreOptions" xsi:type="array">
                                <item name="path" xsi:type="string">Swissup_BreezeAmastyShopby/js/components/amShopbyFilterHideMoreOptions</item>
                            </item>

                            <item name="amShopbyFilterSearch" xsi:type="array">
                                <item name="path" xsi:type="string">Swissup_BreezeAmastyShopby/js/components/amShopbyFilterSearch</item>

                                <item name="import" xsi:type="array">
                                    <item name="amShopbyFiltersSync" xsi:type="string">amShopbyFiltersSync</item>
                                </item>
                            </item>

                            <item name="amShopbyAjax" xsi:type="array">
                                <item name="path" xsi:type="string">Amasty_Shopby/js/amShopbyAjax</item>

                                <item name="import" xsi:type="array">
                                    <item name="amShopbyTopFilters" xsi:type="string">amShopbyTopFilters</item>
                                    <item name="productListToolbarForm" xsi:type="string">productListToolbarForm</item>
                                    <item name="amShopbyFilterAbstract" xsi:type="string">amShopbyFilterAbstract</item>
                                </item>
                            </item>

                            <item name="amShopbyFilterItemDefault" xsi:type="array">
                                <item name="path" xsi:type="string">Amasty_Shopby/js/components/amShopbyFilterItemDefault</item>

                                <item name="import" xsi:type="array">
                                    <item name="amShopbyFilterAbstract" xsi:type="string">amShopbyFilterAbstract</item>
                                    <item name="amShopbyFiltersSync" xsi:type="string">amShopbyFiltersSync</item>
                                </item>
                            </item>

                            <item name="amShopbyFilterSwatch" xsi:type="array">
                                <item name="path" xsi:type="string">Amasty_Shopby/js/components/amShopbyFilterSwatch</item>

                                <item name="import" xsi:type="array">
                                    <item name="amShopbyFilterAbstract" xsi:type="string">amShopbyFilterAbstract</item>
                                    <item name="amShopbyFiltersSync" xsi:type="string">amShopbyFiltersSync</item>
                                </item>
                            </item>

                            <item name="amShopbyFilterCategory" xsi:type="array">
                                <item name="path" xsi:type="string">Amasty_Shopby/js/components/amShopbyFilterCategory</item>

                                <item name="import" xsi:type="array">
                                    <item name="amShopbyFilterAbstract" xsi:type="string">amShopbyFilterAbstract</item>
                                    <item name="amShopbyFiltersSync" xsi:type="string">amShopbyFiltersSync</item>
                                </item>
                            </item>

                            <item name="amShopbyApplyFilters" xsi:type="array">
                                <item name="path" xsi:type="string">Amasty_Shopby/js/amShopbyApplyFilters</item>

                                <item name="import" xsi:type="array">
                                    <item name="amShopbyFilterAbstract" xsi:type="string">amShopbyFilterAbstract</item>
                                </item>
                            </item>
                        </item>
                    </item>
                </argument>
            </arguments>
        </referenceBlock>
    </body>
</page>
